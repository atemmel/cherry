# regular operators
assert 1 == 1
assert true != false
assert "hi" == "hi"
assert 3 != 2 == true

# eq, for lists, queries equality on all elements
assert (eq 0 0)
assert (eq true true true)
assert (eq false false)
assert (eq 0)
assert (eq ha ha ha)
assert (eq)

# lt, for lists, queries rising order
assert 1 < 2
assert (lt 1 2 3)
assert (lt a b c)

# gt, for lists, queries falling order
assert 3 > 2
assert (gt 3 2 1)
assert (gt c b a)

# ne, for lists, queries uniqueness on all elements
assert (ne 3 1 2)
assert (ne true false)
assert (ne 3)
assert (ne a b c)
