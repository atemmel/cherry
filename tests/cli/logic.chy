# regular operators
assert 1 == 1
say true != false
assert true != false
assert "hi" == "hi"
assert 3 != 2 == true

# eq, for lists, queries equality on all elements
assert (eq 0 0)
assert (eq true true true)
assert (eq false false)
assert (eq 0)
assert (eq ha ha ha)
assert (eq)

# lt, for lists, queries rising order
assert (< 1 2 3)
assert (< a b c)

# gt, for lists, queries falling order
assert (> 3 2 1)
assert (> c b a)

# ne, for lists, queries uniqueness on all elements
assert (ne 3 1 2)
assert (ne true false)
assert (ne 3)
assert (ne a b c)
